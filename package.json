{
  "name": "@todesktop/robotjs-prebuild",
  "version": "0.6.18",
  "description": "Node.js Desktop Automation.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "run-script-os",
    "test:darwin:linux": "jasmine test/**/*.js",
    "test-keyboard": "node test/keyboard.js",
    "test:win32": "jasmine test/**/*.js",
    "install": "node-gyp-build",
    "install-debug": "prebuild-install --verbose || node-gyp rebuild --debug",
    "prebuild": "prebuildify --target -t node@15.0.0 -t node@16.0.0 -t node@18.0.0 -t node@20.0.0 -t electron@23.0.0  -t electron@24.0.0  -t electron@25.0.0  -t electron@26.0.0 -t electron@27.0.0",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ToDesktop/robotjs-prebuild.git"
  },
  "keywords": [
    "Automation",
    "GUI",
    "mouse",
    "keyboard",
    "screenshot",
    "image",
    "pixel",
    "desktop",
    "robotjs",
    "screen",
    "recognition",
    "autohotkey",
    "machine",
    "learning",
    "color"
  ],
  "author": "Jason Stallings",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/ToDesktop/robotjs-prebuild/issues"
  },
  "homepage": "https://github.com/ToDesktop/robotjs-prebuild",
  "dependencies": {
    "@todesktop/nan": "^2.17.0",
    "cross-zip": "^4.0.0",
    "node-addon-api": "^5.0.0",
    "node-gyp-build": "^4.5.0",
    "patch-package": "^6.4.7",
    "prebuildify": "^5.0.1"
  },
  "devDependencies": {
    "electron": "^21.0.0",
    "electron-rebuild": "^3.2.9",
    "node-abi": "^3.47.0",
    "node-gyp": "^9.1.0"
  },
  "overrides": {
    "node-gyp": "^9.1.0"
  }
}
